<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->

<!-- Main Site -->
<center>
  <button>
    <a href="/login2" style="text-decoration: none; font-size: 1.2rem"
      >LOGOUT</a
    >
  </button>
</center>
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <a href="/add_user" class="border-shadow">
        <span class="text-gradient">New User <i class="fas fa-user"></i></span>
      </a>
    </div>

    <!-- form handling -->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>UID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Branch</th>
            <th>Section</th>
            <th>OOPS</th>
            <th>DSA</th>
            <th>MATHS</th>
            <th>SOFT SKILLS</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%- include('include/_show') %>
        </tbody>
      </table>
    </form>
  </div>
</main>
<!-- /Main Site -->

<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->
